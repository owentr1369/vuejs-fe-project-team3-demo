{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  name: \"Register\",\n  components: {},\n\n  data() {\n    return {\n      form: {\n        email: \"van100@gmail.com\",\n        password: \"123456\",\n        timezone: \"Asia/Bangkok\",\n        name: \"van\"\n      },\n      formstate: false,\n      is_loading: false\n    };\n  },\n\n  computed: {\n    validation() {\n      const email = {\n        required: this.form.email ? true : false\n      };\n      const password = {\n        required: this.form.password ? true : false,\n        minLength: this.form.password.length >= 6 ? true : false\n      };\n      const name = {\n        required: this.form.name ? true : false\n      };\n      return {\n        form: {\n          email,\n          password,\n          name\n        },\n        valid: email.required && password.required && password.minLength && name.required\n      };\n    }\n\n  },\n  methods: { ...mapActions({\n      register: 'auth/register'\n    }),\n\n    onSubmit() {\n      this.formstate = true;\n      if (this.is_loading || !this.validation.valid) return;\n      this.is_loading = true;\n      this.register(this.form).then(res => {\n        console.log(res);\n        let {\n          status\n        } = res;\n\n        if (status) {\n          this.$router.push({\n            name: 'Dashboard'\n          });\n        }\n\n        this.is_loading = false;\n      }).catch(err => {\n        console.log(err);\n        this.is_loading = false;\n      });\n    }\n\n  },\n  watch: {},\n\n  created() {},\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAuCA;AACA;EACAA,gBADA;EAEAC,cAFA;;EAKAC;IACA;MACAC;QACAC,yBADA;QAEAC,kBAFA;QAGAC,wBAHA;QAIAN;MAJA,CADA;MAOAO,gBAPA;MAQAC;IARA;EAUA,CAhBA;;EAiBAC;IACAC;MACA;QACAC;MADA;MAGA;QACAA,2CADA;QAEAC;MAFA;MAIA;QACAD;MADA;MAGA;QACAR;UACAC,KADA;UAEAC,QAFA;UAGAL;QAHA,CADA;QAMAa;MANA;IAQA;;EApBA,CAjBA;EAuCAC,WACA;MACAC;IADA,EADA;;IAIAC;MACA;MACA;MACA;MACA;QACAC;QACA;UAAAC;QAAA;;QACA;UACA;YACAlB;UADA;QAGA;;QACA;MACA,CATA,EAUAmB,KAVA,CAUAC;QACAH;QACA;MACA,CAbA;IAcA;;EAtBA,CAvCA;EA+DAI,SA/DA;;EAkEAC,WAEA,CApEA;;EAqEAC,WAEA;;AAvEA","names":["name","components","data","form","email","password","timezone","formstate","is_loading","computed","validation","required","minLength","valid","methods","register","onSubmit","console","status","catch","err","watch","created","mounted"],"sourceRoot":"src/modules/auth/views","sources":["register.vue"],"sourcesContent":["<template>\n  <div class=\"vue-app-register-page\">\n    <div class=\"vue-app-register-page-header\">Register Account</div>\n    <div class=\"vue-app-register-page-body\">\n      <form class=\"vue-app-register-page-form\" novalidate @submit.stop.prevent=\"onSubmit\">\n        <div class=\"vue-app-register-page-form-item\">\n          <input type=\"text\" v-model=\"form.name\" placeholder=\"Full name\" />\n          <div class=\"vue-app-register-page-form-error\" v-if=\"formstate\">\n            <template v-if=\"!validation.form.name.required\">Name is required !</template>\n          </div>\n        </div>\n        <div class=\"vue-app-register-page-form-item\">\n          <input type=\"email\" v-model=\"form.email\" placeholder=\"Email\" />\n          <div class=\"vue-app-register-page-form-error\" v-if=\"formstate\">\n            <template v-if=\"!validation.form.email.required\">Email is required !</template>\n          </div>\n        </div>\n        <div class=\"vue-app-register-page-form-item\">\n          <input type=\"password\" v-model=\"form.password\" placeholder=\"Password\" />\n          <div class=\"vue-app-register-page-form-error\" v-if=\"formstate\">\n            <template v-if=\"!validation.form.password.required\">Password is required !</template>\n            <template v-else-if=\"!validation.form.password.minLength\">\n              Password min length 6 character!\n            </template>\n          </div>\n        </div>\n        <div class=\"vue-app-register-page-form-item\">\n          <button type=\"submit\">Create Free Account</button>\n        </div>\n      </form>\n    </div>\n    <div class=\"vue-app-register-page-footer\">\n      Already have an account?\n      <router-link :to=\"{ name: 'Login' }\">Login Now</router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n  export default {\n    name: \"Register\",\n    components: {\n      \n    },\n    data() {\n      return {\n        form: {\n          email: \"van100@gmail.com\",\n          password: \"123456\",\n          timezone: \"Asia/Bangkok\",\n          name: \"van\",\n        },\n        formstate: false,\n        is_loading: false,\n      };\n    },\n    computed: {\n      validation() {\n        const email = {\n          required: this.form.email ? true : false,\n        };\n        const password = {\n          required: this.form.password ? true : false,\n          minLength: this.form.password.length >= 6 ? true : false,\n        };\n        const name = {\n          required: this.form.name ? true : false,\n        };\n        return {\n          form:{\n            email,\n            password,\n            name,\n          },\n          valid : email.required && password.required && password.minLength && name.required\n        };\n      },\n    },\n    methods: {\n      ...mapActions({\n        register : 'auth/register'\n      }),\n      onSubmit() {\n        this.formstate = true;\n        if( this.is_loading || !this.validation.valid ) return \n        this.is_loading = true \n        this.register(this.form).then((res)=>{\n          console.log(res);\n          let { status  } = res \n          if( status ){\n            this.$router.push({\n              name : 'Dashboard'\n            })\n          }\n          this.is_loading = false\n        })\n        .catch((err)=>{\n          console.log(err);\n          this.is_loading = false \n        })\n      },\n    },\n    watch:{\n\n    },\n    created(){\n\n    },\n    mounted(){\n      \n    }\n  };\n</script>\n\n<style lang=\"scss\" scoped>\n  .vue-app-register-page {\n    &-header {\n      font-family: \"Poppins\";\n      font-style: normal;\n      font-weight: 700;\n      font-size: 38px;\n      color: #5b86e5;\n      margin-bottom: 20px;\n    }\n    &-body {\n    }\n    &-form {\n      &-item {\n        display: block;\n        margin-bottom: 20px;\n        position: relative;\n        input {\n          height: 32px;\n          display: block;\n          padding: 8px 12px;\n          background: #fff;\n          border: 1px solid #ccc;\n          font-size: 14px;\n          width: 100%;\n          &:focus {\n            box-shadow: none;\n            outline: none;\n            border-color: #5b86e5;\n          }\n        }\n        button {\n          display: block;\n          width: 100%;\n          padding: 8px 12px;\n          height: 32px;\n          text-align: center;\n          color: #fff;\n          background: #5b86e5;\n          border: 1px solid #5b86e5;\n        }\n      }\n      &-error {\n        color: #f2545d;\n      }\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}